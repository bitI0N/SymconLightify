{
  "elements": [
    { "type": "RowLayout",
      "items": [
        { "type": "ValidationTextBox", "name": "class", "enabled": false, "caption": "Class" },
        { "type": "Image", "name": "green", "visible": false, "image": "data:image/png;base64," },
        { "type": "Image", "name": "red", "visible": false, "image": "data:image/png;base64," }
      ]
    },
    { "type": "RowLayout",
      "items": [
        { "type": "ValidationTextBox", "name": "ID",    "enabled": false, "caption": "Group [id]" },
        { "type": "ValidationTextBox", "name": "UUID",  "enabled": false, "caption": "UUID"       }
      ]
    }
  ],
  "actions": [
    { "type": "RowLayout",
      "items": [
        { "type": "Button", "caption": "On",  "width": "100px", "onClick": "OSR_WriteValue($id, 'STATE', true);"  },
        { "type": "Button", "caption": "Off", "width": "100px", "onClick": "OSR_WriteValue($id, 'STATE', false);" },
        { "type": "Image", "image": "data:image/png;base64,%3D%3D" }
      ]
    },
    { "type": "ExpansionPanel", "name": "devicePanel", "caption": "Assigned devices",
      "items": [
        { "type": "List", "name": "listDevices", "rowCount": 10,
          "columns": [
            { "caption": "Instance ID", "name": "instanceID",  "width": "0px", "visible": false },
            { "caption": "Class",       "name": "class",       "width": "95px" },
            { "caption": "ZLL",         "name": "zigBee",      "width": "60px"  },
            { "caption": "Name",        "name": "name",        "width": "150px" },
            { "caption": "Hue",         "name": "hue",         "width": "50px"  },
            { "caption": "Color",       "name": "color",       "width": "80px"  },
            { "caption": "Temperature", "name": "temperature", "width": "110px" },
            { "caption": "Level",       "name": "level",       "width": "90px"  },
            { "caption": "Saturation",  "name": "saturation",  "width": "90px"  },
            { "caption": "T [ms]",      "name": "transition",  "width": "60px"  },
            { "caption": "Firmware",     "name": "firmware",   "width": "100px" }
          ],
          "values": []
        }
      ]
    }
  ],
  "status": [
    { "code": 102, "icon": "active",   "caption": "Group is active"                  },
    { "code": 104, "icon": "inactive", "caption": "Group is inactive"                },
    { "code": 201, "icon": "error",    "caption": "Lightify socket is not connected" },
    { "code": 299, "icon": "error",    "caption": "Unknown error"                    }
  ]
}